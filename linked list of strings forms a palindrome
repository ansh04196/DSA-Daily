class Solution {
  public:
    bool compute(Node* head) {
        // Your code goes here
        string ans="";
        Node* t = head;
        while(t) {
            ans += t->data;
            t = t->next;
        }
        int i = 0, j = ans.size()-1;
        while(i<j) {
            if(ans[i]!=ans[j])
                return false;
            i++;
            j--;
        }
        return true;
    }
};
